guides(color = F) +
scale_color_brewer(palette = qualitative_pal) +
theme_sm() + theme_sm_axis() +
labs(x = "Year of Reference", y = "Scores Across different Regulatory Measures", title = "Performace drop by Singapore 2004-17 \n",
